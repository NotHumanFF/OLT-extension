{
    "manifest_version": 3,
    "name": "Code Runner Extension",
    "version": "1.0",
    "icons": {
      "16": "icon.png"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }
